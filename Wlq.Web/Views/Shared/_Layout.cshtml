		<title>五里桥</title>
		<link href="@Url.Content("~/content/css/b.css")" rel="stylesheet" type="text/css" />
	</head>

	<body>
		<div class="top">
			<div class="wp header">
				<h1 class="logo"><a href="/"></a></h1>
			</div>
			<div class="wp">
    			@{ Html.RenderAction("Header", "Common"); }
